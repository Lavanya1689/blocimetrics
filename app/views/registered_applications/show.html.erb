<h1>
  <%=@registered_application.name %>
  <small>
       submitted <%= time_ago_in_words(@registered_application.created_at) %> ago
  </small>
</h1>

<div class="row">
  <div class="col-md-8">
    <p><%= @registered_application.url %></p>
    <h3> EVENTS </h3>
      <% @events.each do |ename, evalue| %>
        <ul>
          <li><%= ename %></li> count : <%= evalue.count %>
        </ul>
      <%end%>
  </div>
  <div class="col-md-4">
    <%= link_to "Edit", edit_registered_application_path, class: 'btn btn-success' %>
    <%= link_to "Delete", @registered_application, method: :delete, class: 'btn btn-danger', data: { confirm: 'Are you sure you want to delete this topic?' } %>
  </div>
</div>
